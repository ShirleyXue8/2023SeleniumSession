<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestNG Regression Suite" verbose="2" thread-count="3" parallel="tests">
	
	<test name="Google Test">
	<parameter name="url" value="https://www.google.com/" />
	<parameter name="browser" value="chrome" />
	
		<classes>
			<class name="mytests.GoogleTest" />
		</classes>
	</test>


	<test name="AmazonTest">
	<parameter name="url" value="https://www.amazon.com/" />
	<parameter name="browser" value="firefox" />
		<classes>
			<class name="mytests.AmazonTest" />
		</classes>
	</test>

	

	<test name="OpenCartTest">
	<parameter name="url" value="https://naveenautomationlabs.com/opencart/" />
	<parameter name="browser" value="edge" />
		<classes>
			<class name="mytests.OpenCartTest" />
		</classes>
	</test>




</suite> 
